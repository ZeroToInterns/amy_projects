
public class Tester {
	
	public static void chooseRoom(String newAnimal, boolean sick) {
		if (newAnimal.equals("dog") || newAnimal.equals("Dog")) {
			if (sick == true) {
				System.out.println("We don't accept sick dogs");
			}
			else {
				System.out.println("Its a dog");
			}
		}
		else if (newAnimal.equals("cat") || newAnimal.equals("Cat")) {
			System.out.println("Its a cat");
		}
		else if (newAnimal.equals("zebra") || newAnimal.equals("Zebra")) {
			System.out.println("Its a zebra");
		}
		else {
			System.out.println("We don't accept that animal");
		}
		
	}

	public static void main(String[] args) {
		
		chooseRoom("Dog", false);
		
//		int myInt = 10;
