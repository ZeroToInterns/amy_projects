//		
//		if (temperature >= 77.5 && temperature <= 120) {
//			System.out.println("Its Hot");
//		}
//		else if (temperature >= 15 && temperature < 77.5) {
//			System.out.println("Its livable");
//		}
//		else {
//			System.out.println("Screw this");
//		}
		
	}

}
