
public class Tester {
	
	public static void chooseRoom(String newAnimal) {
		if (newAnimal.equalsIgnoreCase("dog")) {
			System.out.println("Put in dog room");
		}
		else if (newAnimal.equalsIgnoreCase("cat")) {
			System.out.println("Put in cat room");
		}
		else if (newAnimal.equalsIgnoreCase("zebra")) {
			System.out.println("Put in zebra room");
		}
		else {
			System.out.println("Put in other room");
		}
	}

	public static void main(String[] args) {
		
//		int myInt = 10;
//		double mySecondInt = 3.0;
//		double result = myInt * mySecondInt;
//		System.out.println(result);
//		
		double temperature = 77.5;
		
		if (temperature >= 77.5 && temperature <= 120) {
			System.out.println("Its Hot");
		}
		else if (temperature >= 15 && temperature < 77.5) {
			System.out.println("Its liveable");
		}
		else {
			System.out.println("Screw this");
		}
		
		chooseRoom("Dog");
	}

}
