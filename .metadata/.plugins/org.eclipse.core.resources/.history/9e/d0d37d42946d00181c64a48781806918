
public class Tester {

	public static void main(String[] args) {
		Student student = new Student("Micheal", 4.0);
		System.out.println(student.getName());
		student.setName("Mike");
		System.out.println(student.getName());
		System.out.println(student.getGpa());
		student.changeGpa(3.9);
		System.out.println(student.getGpa());
		System.out.println(student.getGpa());
	}

}
