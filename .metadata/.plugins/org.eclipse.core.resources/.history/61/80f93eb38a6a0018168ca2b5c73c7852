
public class ElectronicStore {

	public static int getPrice(String electronic) {
		int price = 0;
		if (electronic.equals("IPad")) {
			price = 300;
		}
		else if (electronic.equals("Macbook")) {
			price = 1000;
		}
		else if (electronic.equals("TV")) {
			price = 800;
		}
		else if (electronic.equals("Phone")) {
			price = 900;
		}
		else {
			price = 10;
		}
		return price;
	}
}
