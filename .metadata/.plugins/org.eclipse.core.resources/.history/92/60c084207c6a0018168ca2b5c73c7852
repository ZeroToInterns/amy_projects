
public class Tester {
	
	public static void chooseRoom(String newAnimal) {
		if (newAnimal.equalsIgnoreCase("dog")) {
			System.out.println("Its dog");
		}
		else if (newAnimal.equalsIgnoreCase("cat")) {
			System.out.println("Its cat");
		}
		else if (newAnimal.equalsIgnoreCase("zebra")) {
			System.out.println("Its zebra");
		}
		else {
			System.out.println("We don't accept that animal");
		}
	}

	public static void main(String[] args) {
		
		chooseRoom("Dog");
		
